{
  "name": "scraper",
  "version": "1.0.0",
  "description": "Web scraper",
  "type": "module",
  "main": "scraper.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 0",
    "dev:cs": "node src/scraper.js cs",
    "dev:nba": "node src/scraper.js nba",
    "dev:football": "node src/scraper.js football",
    "dev:all": "node src/scraper.js all",
    "prod:cs": "node src/scraper.js cs 2>&1 >> /var/log/scraper/scraper.log",
    "prod:nba": "node src/scraper.js nba 2>&1 >> /var/log/scraper/scraper.log",
    "prod:football": "node src/scraper.js football 2>&1 >> /var/log/scraper/scraper.log",
    "prod:all": "node src/scraper.js all 2>&1 >> /var/log/scraper/scraper.log"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oskarrn93/Scraper.git"
  },
  "author": "Oskar Ros√©n",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/oskarrn93/Scraper/issues"
  },
  "homepage": "https://github.com/oskarrn93/Scraper#readme",
  "dependencies": {
    "axios": "^0.19.0",
    "cheerio": "^1.0.0-rc.2",
    "cors": "^2.8.5",
    "eslint": "^6.6.0",
    "express": "^4.17.1",
    "ical-generator": "^1.8.1",
    "mongodb": "^3.3.2"
  },
  "devDependencies": {
    "nodemon": "^1.19.2"
  },
  "build": "npm build",
  "clean": "rm -rf node_modules",
  "reinstall": "npm run clean && npm install",
  "rebuild": "npm run clean && npm install && npm run build"
}
